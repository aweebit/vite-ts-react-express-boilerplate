{
  "name": "vite-ts-react-express-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "client": "vite",
    "server": "cross-env SSR= vite-node -w server/src/index.ts",
    "dev": "concurrently -n client,server -c green,magenta \"npm run client\" \"npm run server\"",
    "lint": "eslint .",
    "build:client": "tsc -b tsconfig/tsconfig.client.json && vite build",
    "build:server": "tsc -b tsconfig/tsconfig.server.json && vite build --ssr",
    "build": "tsc -b && vite build && vite build --ssr",
    "preview": "vite preview",
    "start": "cross-env NODE_ENV=production node dist/server/index.js"
  },
  "dependencies": {
    "express": "^4.21.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.5",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": ">=5.0.0 <5.1.0 || ^5.1.1",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "prettier": "3.5.1",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0",
    "vite-node": "^3.0.6"
  }
}
